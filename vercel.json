{
    "version": 2,
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node",
            "config": {
                "engine": "18.x"
            }
        },
        {
            "src": "client/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "^/users/(.*)",
            "dest": "server/server.js"
        },
        {
            "src": "^/(.*)",
            "dest": "client/public/index.html"
        }
    ]
}